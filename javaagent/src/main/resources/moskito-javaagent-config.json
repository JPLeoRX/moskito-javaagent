{
    "@monitoringClassConfig": [
        {
            "patterns": ["net.anotheria.*"],
            "subsystem": "javaagent",
            "category": "javaagent",
            "nameResolverType": "FULLY_QUALIFIED_CLASS_NAME"
        },
        {
            "patterns": ["org.moskito.demo.daemon.*"],
            "subsystem": "default",
            "category": "demo",
            "nameResolverType": "ABBREVIATION"
        }
    ],
    "mode": "PROFILING",
    "startMoskitoBackend": true,
    "moskitoBackendPort": 9411
}
