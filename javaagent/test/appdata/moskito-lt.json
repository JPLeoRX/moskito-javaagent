{
  "@monitoringClassConfig": [
    {
      "patterns": [
        "org.primefaces.*",
        "org.apache.commons.fileupload.FileItem"
      ],
      "producerId": "Primefaces",
      "subsystem": "default",
      "category": "web"
    },
    {
      "patterns": [
        "org.apache.*"
      ],
      "producerId": "Apache",
      "subsystem": "default",
      "category": "web"
    }
  ],
  "classesToInclude": [
    "javax.*",
    "sun.*",
    "net.anotheria",
    "org.aspectj",
    "org.moskito.javaagent.instrumentations.sample.EchoTestWithoutMonitoring"
  ],

  "mode" : "PROFILING",
  "startMoskitoBackend": false,
  "moskitoBackendPort": 10000
}